{
  "name": "ada-lib",
  "description": "工具集合",
  "version": "0.0.5",
  "license": "MIT",
  "main": "lib/ada-lib.umd.min.js",
  "scripts": {
    "build": "npm run build:lib;npm run build:doc",
    "lint": "vue-cli-service lint",
    "build:doc": "vuese gen;vuepress build docs",
    "build:lib": "node scripts/build.js",
    "dev": "vuese gen;vuepress dev --no-cache docs",
    "prepack": "npm run build:lib",
    "test:unit": "vue-cli-service test:unit"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "vue": "^2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-jest": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "@vuepress/plugin-register-components": "^1.0.0-alpha.0",
    "@vuese/cli": "^2.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "markdown-it-include": "^1.0.0",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2",
    "vuepress": "^1.0.0-alpha.32"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
